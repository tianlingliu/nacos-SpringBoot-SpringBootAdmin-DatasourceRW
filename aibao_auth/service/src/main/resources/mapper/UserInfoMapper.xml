<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.aibao.claims.auth.mapper.UserInfoMapper">
    <resultMap id="BaseResultMap" type="com.aibao.claims.auth.mapper.entity.UserInfo">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="name" property="name" jdbcType="VARCHAR"/>
        <result column="age" property="age" jdbcType="VARCHAR"/>

    </resultMap>


    <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Integer">
    select * from user_info where id = #{id,jdbcType=INTEGER}
  </select>


    <insert id="insert" parameterType="com.aibao.claims.auth.mapper.entity.UserInfo">
      insert into user_info(name,age) values (#{name},#{age})
    </insert>


</mapper>